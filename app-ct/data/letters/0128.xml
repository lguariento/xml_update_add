                    <persName ref="pe0232">Thomas Pennant</persName>
                    <date when="1773-03-04"/>
                    <placeName ref="pldb.xml#pl0699">London</placeName>
                </correspAction>
                <correspAction type="received">
                    <persName ref="pe0228">George Paton</persName>
