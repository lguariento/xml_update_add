            <correspDesc>
                <correspAction type="sent">
                    <persName ref="pe0232">Thomas Pennant</persName>
                    <date when="1774-05-17"/>
                    <placeName ref="pl0001">Downing</placeName>
                </correspAction>
                <correspAction type="received">
                    <persName ref="pe0228">George Paton</persName>
                    <placeName ref="pl0696">Custom House, Edinburgh</placeName>
                </correspAction>
            </correspDesc>
